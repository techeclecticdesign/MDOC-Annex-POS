{
  "name": "mdoc_annex_pos",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "vitest run --passWithNoTests",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "format": "prettier --write . --ignore-path .prettierignore",
    "cargo-fmt": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "cargo-fmt:check": "cargo fmt --manifest-path src-tauri/Cargo.toml -- --check",
    "cargo-lint": "cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "cargo-test": "cargo test --manifest-path src-tauri/Cargo.toml",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "jsdom": "^26.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.1",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
